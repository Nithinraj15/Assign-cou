package CinditionalStatement;

public class IsoscelesStartTriangle {
	public static void main(String[] args) {
		int n =5;
		for(int i=1; i<=n;i++) {
			for(int k=1; k<=5 ; k--) {
				System.out.print(" ");
			}
			for(int j=i*2-1;j>0;j--) {
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
