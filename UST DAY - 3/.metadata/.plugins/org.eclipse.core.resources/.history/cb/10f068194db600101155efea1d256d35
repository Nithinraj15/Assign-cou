package CinditionalStatement;

public class DemonstrateBreak {
	public static void main(String[] args) {
		for(int i=1;i<=20;i++) {
			if(i==10) {
				break;
			}
			System.out.println("Processing number: " + i);
		}
		System.out.println("Loop 1 finished");
		for(int i=11;i<=20;i++) {
			System.out.println("Processing number: " + i);
		}
	}
}
