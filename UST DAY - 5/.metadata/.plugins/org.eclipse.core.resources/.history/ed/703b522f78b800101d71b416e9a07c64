package array;

public class ArrayInitialization_IPL {
	public static void main(String args[]) {
		String[] teamNames = { "Mumbai Indians", "Chennai Super Kings", 
							   "Royal Challengers Bangalore", "Kolkata Knight Riders", 
			                 	"Sunrisers Hyderabad", "Delhi Capitals",
				                "Punjab Kings", "Rajasthan Royals"};
	int[] teamWins = {10,9,7,8,6,9,5,7};
	for(int i=0;i<teamNames.length;i++) {
		System.out.println(teamNames[i] + ": " + teamWins[i]);
	}
	}
}
