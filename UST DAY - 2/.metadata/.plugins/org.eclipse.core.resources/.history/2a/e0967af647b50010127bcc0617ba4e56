package DataType;
class void changePrimitive() {
	int value = 50;
}
changePrimitive(int value){
	this.value = value;
}
public class CopyValue {
	public static void main(String args[]) {
		int num1 = 10;
		int num2 = num1;
		num2 = 20;
		changePrimitive cp = changePrimitive();
		cp.changePrimitive(num1);
		System.out.println(num1);
	}
}
