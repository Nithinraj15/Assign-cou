package Challenges;

public class Find_length {
	public static void main(String args[]) {
		int a = 12345;
		float b = 12345.6f;
		String str = "NITHINRAJ";
		Integer num = a;
		System.out.println("Integer length : " + num.SIZE);
		System.out.println("String length : " + str.length());
		System.out.println();
	}
}
